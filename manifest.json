{
  "name": "Maji Merge for GitHub",
  "version": "0.0.2",
  "manifest_version": 3,
  "description": "GitHubのPull Request内マージボタンに確認を挟んでくれる君。",
  "permissions": ["scripting"],
  "host_permissions": ["https://github.com/"],
  "background": {
    "service_worker": "src/background.js"
  },
  "icons" : {
    "16" : "icon16.png",
    "48" : "icon48.png",
    "128" : "icon128.png"
  }
}